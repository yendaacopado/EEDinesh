<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>copado__Merge_Strategy__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <inlineHelpText>When a conflicted file cannot be semantically merged during an Auto Resolve, the entire file is overwritten by default. You can choose to overwrite only the conflicting lines and merge the mergeable lines; Copado uses the &quot;Theirs&quot; git merge strategy.</inlineHelpText>
    <label>Auto Resolve Conflict Strategy</label>
    <required>false</required>
    <trackHistory>true</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Default</fullName>
                <default>true</default>
                <label>Overwrite file</label>
            </value>
            <value>
                <fullName>Theirs</fullName>
                <default>false</default>
                <label>Overwrite conflicting lines only</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
